{
    "application": {
        "version": "6.0.1"
    },
    "canvas": {
        "defaultWidth": 400,
        "defaultHeight": 500,
        "defaultOffsetX": 50,
        "defaultOffsetY": 50,
        "defaultRotation": 0,
        "defaultZoom": 1,
        "paddingX": 300,
        "paddingY": 300
    },
    "viewport": {
        "cursor": "crosshair"
    },
    "geometry": {
        "stroke": {
            "dark": "#ffffff",
            "light": "#000000"
        },
        "selectedStroke": {
            "dark": "#debfff",
            "light": "#6756a0"
        },
        "width": 1,
        "selectedWidth": 2,
        "radius": 1.5,
        "selectedRadius": 3.5,
        "baseHit": 20
    },
    "ruler": {
        "majorLength": 10,
        "minorLength": 8,
        "subLength": 3,
        "majorInterval": 16.4,
        "minorDivisions": 2,
        "subMinorDivisions": 5,
        "thickness": 20
    },
    "document": {
        "default_name": "Untitled Project",
        "default_light_background": "#F9FAFB",
        "default_dark_background": "#1e1e1f"
    },
    "modes": {
        "drag_lasso_threshold": 4,
        "debug_select": false
    }
}